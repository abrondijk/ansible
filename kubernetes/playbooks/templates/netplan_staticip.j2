network:
  ethernets:
    eth0:
      match:
        macaddress: {{ hostvars[inventory_hostname].ansible_default_ipv4.macaddress }}
      set-name: eth0
      addresses:
      - {{ ansible_host }}/24
      routes:
      - to: default
        via: 10.0.20.1
      nameservers:
        addresses:
        - 10.0.20.190
        search: []
  version: 2